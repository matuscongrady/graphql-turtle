{
  "name": "graphql-turtle",
  "author": "m.congrady@gmail.com",
  "engines": {
    "node": ">= 8.0.0",
    "yarn": ">= 1.7.0"
  },
  "license": "MIT",
  "scripts": {
    "dev:client": "npx cross-env NODE_ENV=development webpack-dev-server",
    "dev:api": "env-cmd api/.env.dev ts-node-dev --no-notify api/index.ts",
    "analyze": "npx rimraf dist && npx cross-env NODE_ENV=production APP_ENV=analyze webpack --hide-modules",
    "docker:build": "yarn build:api && yarn build:client && docker build -t turtle .",
    "docker:run": "docker run -v ${PWD}/turtle-config.json:/app/turtle-config.json -p 4000:4000 turtle",
    "build:client": "npx rimraf dist-client && npx cross-env NODE_ENV=production webpack",
    "build:api": "npx rimraf dist-api && npx tsc --outDir dist-api -p 'tsconfig.api.json'",
    "test:docker:dist": "yarn docker:build && yarn docker:run"
  },
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "@reach/router": "^1.2.1",
    "@types/validator": "^9.4.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "emotion": "^9.2.12",
    "env-cmd": "^8.0.2",
    "express": "^4.16.4",
    "fetch-suspense": "^1.0.1",
    "fs-extra": "^7.0.0",
    "graphql": "^14.0.2",
    "graphql-request": "^1.8.2",
    "lodash": "^4.17.10",
    "morgan": "^1.9.1",
    "react": "^16.7.0-alpha.0",
    "react-data-fetching": "^0.2.4",
    "react-dom": "^16.7.0-alpha.0",
    "react-hanger": "^1.0.8",
    "react-hot-loader": "^4.3.12",
    "react-json-editor-ajrm": "^2.5.8",
    "react-syntax-highlighter": "^10.0.0",
    "react-use": "^4.8.0",
    "signale": "^1.3.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
